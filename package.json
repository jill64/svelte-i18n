{
  "name": "@jill64/svelte-i18n",
  "version": "2.0.0",
  "description": "üåç i18n toolkit for SvelteKit",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/svelte-i18n/issues",
  "homepage": "https://github.com/jill64/svelte-i18n#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./app": {
      "types": "./dist/app/index.d.ts",
      "svelte": "./dist/app/index.js",
      "default": "./dist/app/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/svelte-i18n.git",
    "image": "https://opengraph.githubassets.com/1825d44ae1432137d2edc3804bb6a40e5f8a4a384ccfe0d6603082ccdc0d1429/jill64/svelte-i18n"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "i18n",
    "language",
    "library",
    "localization",
    "svelte",
    "sveltekit"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx psvx",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^5.0.0"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-svelte": "2.0.2",
    "@jill64/npm-demo-layout": "2.0.6",
    "@jill64/playwright-config": "2.4.2",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "2.0.1",
    "@jill64/universal-sanitizer": "1.3.6",
    "@playwright/test": "1.49.1",
    "@sveltejs/adapter-cloudflare": "5.0.0",
    "@sveltejs/kit": "2.15.2",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "svelte": "5.16.0",
    "svelte-page-tab": "2.0.4",
    "typescript": "5.7.2",
